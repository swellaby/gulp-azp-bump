{
  "files.exclude": {
    "npm-debug.log": true,
    ".testresults": true,
    ".coverage": true,
    "node_modules": true,
    ".nyc_output": true,
    "*.tgz": true
  },
  "cSpell.enabled": true,
  "cSpell.enabledLanguageIds": [
    "javascript",
    "json",
    "markdown",
    "plaintext",
    "properties",
    "text"
  ],
  "cSpell.dictionaries": [
    "companies",
    "en_us",
    "filetypes",
    "misc",
    "node",
    "npm",
    "softwareTerms"
  ],
  "files.associations": {
    "**/.azure-pipelines/**": "azure-pipelines"
  }
}
